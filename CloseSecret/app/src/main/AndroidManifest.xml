<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.closesecret">

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.closesecret.atys.AtyHome"></activity>
        <activity android:name="com.example.closesecret.atys.AtyLogin"></activity>
        <activity android:name="com.example.closesecret.atys.AtyTimeline"></activity>
        <activity android:name="com.example.closesecret.atys.AtyMessage"></activity>
        <activity android:name="com.example.closesecret.atys.AtyPublish"></activity>
    </application>

</manifest>